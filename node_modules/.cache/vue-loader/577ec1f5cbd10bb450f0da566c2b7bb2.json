ass=\"navbar-menu\">\n        <div class=\"navbar-start\">\n            <router-link to=\"/\" class=\"navbar-item\">Home</router-link>\n            <router-link to=\"/about\" class=\"navbar-item\">About</router-link>\n            <router-link to=\"/booking\" class=\"navbar-item\">Booking schedule</router-link>\n        </div>\n        <div class=\"navbar-end\">\n            <div class=\"navbar-item\">\n                <div class=\"buttons\">\n                    <!-- Check that the SDK client is not currently loading before accessing is methods -->\n                    <div v-if=\"!$auth.loading\">\n                        <!-- show login when not authenticated -->\n                        <a v-if=\"!$auth.isAuthenticated\" @click=\"login\" class=\"button is-dark\"><strong>Sign in</strong></a>\n                        <!-- show logut when authenticated -->\n                        <a v-if=\"$auth.isAuthenticated\" @click=\"logout\" class=\"button is-dark\"><strong>Log out</strong></a>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</nav>\n</template>\n<script>\nexport default {\n    name: 'Nav',\n    methods: {\n        // Log the user in\n        login() {\n            this.$auth.loginWithRedirect();\n        },\n        // Log the user out\n        logout() {\n            this.$auth.logout({\n                returnTo: window.location.origin\n            });\n        }\n    }\n}\n</script>\n<style lang=\"scss\" scoped>\n    nav {\n        margin-top: 25px;\n        margin-bottom: 30px;\n    \n    a {\n        font-weight: bold;\n        color: #2c3e50;\n        &.router-link-exact-active {\n            color: #d88d00;\n        }\n    }\n}\n\n</style>"]}]}